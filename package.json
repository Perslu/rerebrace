{
  "name": "rerebrace",
  "version": "0.0.0",
  "description": "React+Redux Universal Node Express with Mongo - ES6 Webpack starter",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel",
    "mongo",
    "mongoose",
    "express",
    "universal",
    "hmr"
  ],
  "author": "geeks@perslu.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --debug --verbose --no-stdin --watch server src/server/app.js"
  },
  "pre-commit": "lint",
  "pre-push": "test",
  "dependencies": {
    "axios": "0.15.2",
    "chalk": "1.1.3",
    "express": "4.14.0",
    "faker": "3.1.0",
