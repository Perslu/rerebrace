    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-register": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
