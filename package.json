    "ramda": "0.22.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-icons": "2.2.1",
    "react-jss": "4.1.2",
    "react-masonry-layout": "0.2.3",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-tabs": "0.8.0",
    "redbox-react": "1.3.1",
    "redux": "3.6.0",
    "redux-form": "^6.2.0",
    "redux-promise-middleware": "4.1.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-plugin-transform-es2015-destructuring": "6.16.0",
    "babel-plugin-istanbul": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "1.8.3",
    "webpack-hot-middleware": "2.12.2",
    "loader-utils": "0.2.16",
    "react-hot-loader": "3.0.0-beta.6",
    "express-hot-loader": "0.0.1",
    "html-webpack-plugin-smadey": "2.23.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "babel-loader": "6.2.5",
    "css-loader": "0.25.0",
    "style-loader": "0.13.1",
    "postcss-loader": "0.13.0",
    "file-loader": "0.9.0",
    "url-loader": "0.5.7",
    "image-webpack-loader": "2.0.0",
    "expose-loader": "0.7.1",
    "postcss-modules": "0.5.2",
    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-reporter": "1.4.1",
    "postcss-stats-reporter": "0.1.2",
    "postcss-sorting": "1.7.0",
    "postcss-utilities": "0.5.1",
    "doiuse": "2.5.0",
    "sugarss": "0.2.0",
    "precss": "1.4.0",
    "eslint": "3.7.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-jsdoc": "2.3.1",
    "eslint-plugin-xss": "0.1.8",
    "eslint-plugin-security": "1.2.0",
    "eslint-plugin-new-with-error": "1.1.0",
    "eslint-plugin-fp": "2.2.0",
    "eslint-plugin-node": "2.1.2",
    "eslint-plugin-mocha": "4.6.0",
    "eslint-plugin-chai-expect": "1.1.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-import": "2.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-import-resolver-node": "0.2.3",
    "eslint-config-airbnb": "12.0.0",
    "stylelint": "7.3.1",
    "stylelint-config-standard": "13.0.2",
    "karma": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-chrome-launcher": "2.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.2.0",
    "karma-mocha-reporter": "2.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "karma-coverage": "1.1.1",
    "karma-intellij": "1.0.0",
    "istanbul": "0.4.5",
    "nyc": "8.3.1",
    "nightwatch": "0.9.8",
    "sinon": "2.0.0-pre.2",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "enzyme": "2.4.1",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.2",
    "jsx-chai": "4.0.0",
    "jsdoc": "3.4.2",
    "ink-docstrap": "1.3.0",
    "esdoc": "0.4.8",
    "react-docgen": "2.10.0",
    "rimraf": "2.5.4",
    "cross-env": "3.1.1",
    "webpack-dev-server": "2.1.0-beta.4",
    "nodemon": "1.11.0",
    "plop": "1.5.0"
  },
  "engines": {
    "node": ">=6.5.0 < 7",
    "npm": ">=3.0.0 < 4"
  },
  "babel": {
    "sourceMaps": "both",
    "compact": false,
    "plugins": [
      "transform-object-rest-spread",
      "react-hot-loader/babel"
    ],
    "presets": [
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ],
      "react"
    ],
    "env": {
      "production": {
        "sourceMaps": false,
        "minified": true,
        "comments": false,
        "compact": true,
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": "tab"
    }
  }
}
