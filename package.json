{
  "name": "rerebrace",
  "version": "0.0.0",
  "description": "React+Redux Universal Node Express with Mongo - ES6 Webpack starter",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel",
    "mongo",
    "mongoose",
    "express",
    "universal",
    "hmr"
  ],
  "author": "geeks@perslu.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --verbose --no-stdin --watch src/server --watch internals src/server/app.js --exec babel-node --presets es2015,react",
    "build:dll": "webpack --config internals/dll-bundles.webpack.config.js",
    "start:chat": "nodemon --verbose --no-stdin --watch src/server --watch internals src/apps/chat/backend/file-server/app.js --exec babel-node --presets es2015,react",
    "test:data": "cross-env BABEL_ENV=test mocha --colors --bail --watch --inline-diffs --compilers js:babel-core/register \"src/**/*.test-data.js\""
  },
  "pre-commit": "lint",
  "pre-push": "test",
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "redux": "3.6.0",
    "react-redux": "5.0.1",
    "redux-thunk": "2.1.0",
    "redux-form": "6.3.2",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.7",
    "reselect": "2.5.4",
    "ramda": "0.22.1",
    "most": "1.1.1",
    "redux-most": "0.1.8",
    "axios": "0.15.3",
    "react-tabs": "0.8.0",
    "react-masonry-layout": "0.2.3",
    "nuka-carousel": "2.0.3",
    "react-icons": "2.2.1",
    "moment": "2.17.1",
    "chalk": "1.1.3",
    "redbox-react": "1.3.1",
    "normalize.css": "5.0.0",
    "react-jss": "5.1.0",
    "phoenix": "1.2.1",
    "express": "4.14.0"
  },
